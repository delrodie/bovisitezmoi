{% extends 'base.html.twig' %}

{% block body %}
    <h1>Famille</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ famille.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ famille.libelle }}</td>
            </tr>
            <tr>
                <th>Nombreproduit</th>
                <td>{{ famille.nombreProduit }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ famille.slug }}</td>
            </tr>
            <tr>
                <th>Publiepar</th>
                <td>{{ famille.publiePar }}</td>
            </tr>
            <tr>
                <th>Modifiepar</th>
                <td>{{ famille.modifiePar }}</td>
            </tr>
            <tr>
                <th>Publiele</th>
                <td>{{ famille.publieLe }}</td>
            </tr>
            <tr>
                <th>Modifiele</th>
                <td>{{ famille.modifieLe }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('backend_famille_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('backend_famille_edit', { 'id': famille.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
