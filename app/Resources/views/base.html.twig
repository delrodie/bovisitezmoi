<!--[if IE]><html class="ie" lang="fr"> <![endif]-->

{% block javascripts %}
    <script src="{{ absolute_url(asset('js/jquery.min.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('js/bootstrap.min.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('js/jquery.sticky.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('js/parallax.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('js/jquery.flexslider-min.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('js/jquery.jcarousel.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('js/fancySelect.js')) }}"></script>
    <script src="{{ absolute_url(asset('js/animate.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('js/myscript.js')) }}" type="text/javascript"></script>
{% endblock %}
<script>
    if (top != self) top.location.replace(self.location.href);
</script>

</body>

</html>
